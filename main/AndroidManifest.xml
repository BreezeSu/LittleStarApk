<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.breeze.project1.littlestar">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:name=".LittleStarApplication"
        android:allowBackup="true"
        android:icon="@drawable/littlestar"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".activity.PhotoListActivity">
        </activity>
        <activity
            android:name=".activity.PhotoDetailActivity"
            android:label="@string/title_activity_photo_detail"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
    </application>

</manifest>